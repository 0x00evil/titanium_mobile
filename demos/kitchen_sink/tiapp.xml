<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://ti.appcelerator.org">
<id>com.nolanwright.mobilefun</id>
<name>mobilefun</name>
<version>1.0</version>
<icon>app_icon.png</icon>
    <windows>
        <window>
            <id>Media</id>
            <url>index.html</url>
            <backgroundColor>#fff</backgroundColor>
<icon>appicon.png</icon>
			<orientation>either</orientation>
        </window>
        <window>
            <id>UI</id>
            <url>ui.html</url>
			<title>UI</title>
            <backgroundColor>#fff</backgroundColor>
<icon>appicon.png</icon>
			<orientation>either</orientation>

        </window>
        <window>
            <id>Info</id>
            <url>info.html</url>
			<title>Data</title>
            <backgroundColor>#fff</backgroundColor>
<icon>appicon.png</icon>
			<orientation>either</orientation>

        </window>
        <window>
            <id>Other</id>
            <url>other.html</url>
            <backgroundColor>#fff</backgroundColor>
<icon>appicon.png</icon>
			<orientation>either</orientation>

        </window>

    </windows>
    <property name="ti.android.flushjscache" type="bool">true</property>
    <property name="ti.android.minifyjs" type="bool">false</property>
    <property name="ti.android.watchlog" type="bool">false</property>
    <!-- ti.deploytype will be set by the development toolkit -->
    <property name="ti.deploytype">development</property>

</ti:app>
